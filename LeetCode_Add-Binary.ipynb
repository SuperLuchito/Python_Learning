    "a = \"1010\"\n",
    "b = \"1011\"\n",
    "\n",
    "\n",
    "def addBinary(a, b):\n",
    "    decimal_a = int(a, 2)\n",
    "    decimal_b = int(b, 2)\n",
    "\n",
    "    decimal_sum = decimal_a + decimal_b\n",
    "\n",
    "    binary_sum = bin(decimal_sum)[2:]\n",
    "\n",
    "    return binary_sum\n",
    "\n",
    "\n",
    "print(addBinary(a, b))"
   ]
